<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="331.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.spo.view.FileView">
   <children>
      <ChoiceBox id="Filetype_Selector" fx:id="ComboBox" layoutX="14.0" layoutY="14.0" prefWidth="150.0" />
